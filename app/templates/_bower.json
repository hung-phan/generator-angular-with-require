{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "dependencies": {
<% if (cssFramework === 'SASSBootstrap') { %>
    "sass-bootstrap": "~3.0.2",
<% } %>
<% if (cssFramework === 'NativeBootstrap') { %>
    "bootstrap": "~3.0.3",
<% } %>
<% if (includeUnderscore) { %>
    "underscore": "~1.5.2",
<% } %>
    "angular": "~1.2.9",
    "angular-resource": "~1.2.9",
    "angular-route": "~1.2.9",
<% if (includeUIBootstrap) { %>
    "angular-ui-bootstrap": "~0.10.0",
<% } %>
    "jquery": "~2.0.3",
    "requirejs": "~2.1.10",
    "modernizr": "~2.7.1"  
  }
}

